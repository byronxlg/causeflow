# Environment Configuration
# Set to 'true' for local API, 'false' or omit for Appwrite
VITE_USE_LOCAL_API=false

# Local API Configuration (used when VITE_USE_LOCAL_API=true)
VITE_API_HOST=http://localhost:3000

# Appwrite Configuration (used when VITE_USE_LOCAL_API=false)
VITE_APPWRITE_PROJECT_ID=your_project_id_here
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
VITE_APPWRITE_API_KEY=your_api_key_here